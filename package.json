{
  "name": "ng2-panzoom",
  "version": "9.0.0",
  "author": "Tim Underhay",
  "license": "MIT",
  "homepage": "https://github.com/kensingtontech/ng2-panzoom",
  "repository": {
    "type": "git",
    "url": "https://github.com/KensingtonTech/ng2-panzoom.git"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildpack": "rm -rf dist/ && ng build --prod && cd dist/ng2-panzoom && npm pack .",
    "buildpublish": "rm -rf dist/ && ng build --prod && cd dist/ng2-panzoom && npm publish .",
    "builddemotest": "VER=`grep version projects/ng2-panzoom/package.json | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]'` && npm run buildpack && cp dist/ng2-panzoom/ng2-panzoom-*.tgz ~/src/ng2-panzoom-demo && cd ~/src/ng2-panzoom-demo && npm install ng2-panzoom-${VER}.tgz"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^9.0.1",
    "@angular/compiler": "^9.0.1",
    "@angular/core": "^9.0.1",
    "@angular/platform-browser": "^9.0.1",
    "@angular/platform-browser-dynamic": "^9.0.1",
    "core-js": "^2.5.4",
    "ng-packagr": "^9.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular-devkit/build-ng-packagr": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "^9.0.1",
    "@angular/language-service": "^9.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.32",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "^3.5.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.16.0",
    "typescript": "~3.7.5"
  }
}
